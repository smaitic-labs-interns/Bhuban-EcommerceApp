{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "Components/*": ["src/components/*"],
      "public/*": ["src/public/*"],
      "Pages/*": ["src/pages/*"],
      "Routes/*": ["src/routes/*"],
      "Hooks/*": ["src/hooks/*"],
      "Utils/*": ["src/utils/*"],
      "Api/*": ["src/api/*"],
      "Datas/*": ["src/datas/*"],
      "Redux": ["src/redux/*"],
      "Actions": ["src/redux/actions/*"],
      "Modules": ["src/modules/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
